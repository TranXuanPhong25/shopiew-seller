{
  "repositoryUrl": "https://github.com/TranXuanPhong25/shopiew-seller.git",
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    ["@semantic-release/release-notes-generator", {
      "preset": "conventionalcommits",
      "presetConfig": {
        "types": [
          { "type": "feat", "section": "ğŸš€ Features" },
          { "type": "fix", "section": "ğŸ› Bug Fixes" },
          { "type": "docs", "section": "ğŸ“ Documentation" },
          { "type": "perf", "section": "âš¡ Performance" },
          { "type": "refactor", "section": "ğŸ”¨ Technical Changes" },
          { "type": "build", "section": "ğŸ”¨ Technical Changes" },
          { "type": "ci", "section": "ğŸ”¨ Technical Changes" }
        ]
      }
    }],
    ["@semantic-release/changelog", {
      "changelogFile": "CHANGELOG.md"
    }],
    "@semantic-release/github",
    ["@semantic-release/git", {
      "assets": ["CHANGELOG.md"],
      "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }]
  ]
}